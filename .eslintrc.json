{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-shadow": 0,
      "no-param-reassign": 0,
      "eol-last": 0,
      "arrow-parens": [2,"always"],
      "import/extensions":[ 1,"ignorePackages"],
      "import/no-extraneous-dependencies": "off"
    },
    "settings": {
        "import/resolver": "webpack"
    }
  }